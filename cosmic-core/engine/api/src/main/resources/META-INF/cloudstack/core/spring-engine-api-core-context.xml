<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
>

    <bean id="clusterRestService" class="com.cloud.engine.rest.service.api.ClusterRestService" />
    <bean id="dataDisk" class="com.cloud.engine.subsystem.api.storage.type.DataDisk" />
    <bean id="podRestService" class="com.cloud.engine.rest.service.api.PodRestService" />
    <bean id="iso" class="com.cloud.engine.subsystem.api.storage.type.Iso" />
    <bean id="networkRestService" class="com.cloud.engine.rest.service.api.NetworkRestService" />
    <bean id="provisioningServiceImpl" class="com.cloud.engine.service.api.ProvisioningServiceImpl" />
    <bean id="rootDisk" class="com.cloud.engine.subsystem.api.storage.type.RootDisk" />
    <bean id="virtualMachineRestService" class="com.cloud.engine.rest.service.api.VirtualMachineRestService" />
    <bean id="volumeRestService" class="com.cloud.engine.rest.service.api.VolumeRestService" />
    <bean id="volumeTypeHelper" class="com.cloud.engine.subsystem.api.storage.type.VolumeTypeHelper" />
    <bean id="zoneRestService" class="com.cloud.engine.rest.service.api.ZoneRestService" />

</beans>
